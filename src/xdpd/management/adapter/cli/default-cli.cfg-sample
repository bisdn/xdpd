enable
# set debuglevel <class|all> <level> (class and level names are not case sensitive)
set debuglevel ciosrv emergency
set debuglevel cthread warn
configure terminal
openflow
# do not change the order of parameters (parameters can be omitted from rtl)
create dp0 1544 3 1
configure dp0
attach_port veth0 1
attach_port veth2 2
attach_port veth4 3
attach_port veth6 4
# ip:port is the current default if it is omitted
# connect 127.0.0.1:6632
quit
